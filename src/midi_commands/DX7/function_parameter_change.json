  "status_byte": 240,
  "parameters": [
    {"name": "Device Number", "min": 0, "max": 15, "default": 0},
    {"name": "Function Number", "min": 0, "max": 127, "default": 0},
    {"name": "Value", "min": 0, "max": 127, "default": 0}
  ],
  "template": "F0 43 1{device_number} 03 {function_number} {value} F7"
}